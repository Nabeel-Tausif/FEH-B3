void PerformanceTest1(float degrees)
{

    StartProgram(CdS_cell);
    MoveSwitch(120,50, FrontSwitch);
    MoveTime(0, IDEALPOWER, 500);
    MoveTime(270, IDEALPOWER, 2500);
    MoveSwitch(0, 80, LeftSwitch);
    MoveSwitch(90,50, FrontSwitch);
    MoveTime(270, IDEALPOWER, 800);
    ChangeOrientation(1022);
    MoveTime(315, 75, 5000);
}
